try:
    x=input().strip()
    y=input().strip()
    a=float(x)
    b=float(y)
    dr=a / b
    mr=int(a%b)
    print(f"Division result: {dr} \nModulo result: {mr}")
except ZeroDivisionError:
    print("Error: Cannot divide or modulo by zero.")
except ValueError:
    print("Error: Non-numeric input provided.")
