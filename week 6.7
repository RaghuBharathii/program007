# Input handling
n = int(input(""))
arr = []
for i in range(n):
    arr.append(int(input("".format(i+1))))
element_to_search = int(input(""))

# Finding locations and counting occurrences
locations = []
occurrences = 0
for i, num in enumerate(arr):
    if num == element_to_search:
        locations.append(i + 1)  # Adjusting index to start from 1
        occurrences += 1

# Outputting results
if occurrences == 0:
    print(f"{element_to_search} is not present in the array.")
else:
    for loc in locations:
        print(f"{element_to_search} is present at location {loc}.")
    print(f"{element_to_search} is present {occurrences} times in the array.")
