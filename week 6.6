# Input handling
n = int(input())
arr = []
for _ in range(n):
    arr.append(int(input()))

# Counting frequencies
frequency = {}
for num in arr:
    if num in frequency:
        frequency[num] += 1
    else:
        frequency[num] = 1

# Outputting frequencies
for num, freq in frequency.items():
    print(f"{num} occurs {freq} times")
